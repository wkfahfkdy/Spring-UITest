<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.kjm.uitest.member.map.MemberMapper">
	
	<select id="memberLogin" resultType="co.kjm.uitest.member.vo.MemberVO">
		select	id, password
		from 	member
		where	id = #{id}
		and		password = #{password}
	</select>
	
</mapper>